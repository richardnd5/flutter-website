    "version": "0.2.0",
    "configurations": [
        {
            "name": "flutter_website",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "flutter_website (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "Web Server",
            "request": "launch",
            "type": "dart",
            "program": "./lib/main.dart",
            "args": [
                "-d",
                "web-server",
                "--web-port",
                "8000"
            ]
        }
    ]
}